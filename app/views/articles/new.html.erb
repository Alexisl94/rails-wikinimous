<h1>New Article</h1>
<%= form_with model: @article do |f| %>
<div class="form-control formulaire">
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
  </div>
<% end %>
<div class="btn btn-danger red-button">
<%= link_to 'Back', articles_path %>
</div>
